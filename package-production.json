{
  "name": "coreflame-consciousness-platform",
  "version": "2.0.0",
  "description": "AI Consciousness Platform serving dual functions: portfolio showcase and prototype foundation for AstralVibe.ca",
  "main": "server/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "start": "NODE_ENV=production tsx server/index.ts",
    "start:production": "NODE_ENV=production PM2_HOME=/opt/coreflame/.pm2 pm2-runtime start ecosystem.config.js --env production",
    "start:cluster": "NODE_ENV=production PM2_HOME=/opt/coreflame/.pm2 pm2-runtime start ecosystem.config.js --env cluster",
    "start:optimized": "NODE_ENV=production node --max-old-space-size=1024 --optimize-for-size dist/server/index.js",
    "build": "tsc -p tsconfig.server.json && vite build",
    "build:production": "NODE_ENV=production npm run build && npm run optimize",
    "optimize": "node scripts/optimize-production.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint server client/src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "stop": "pm2 stop all && pm2 delete all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "monitor": "pm2 monit"
  },
  "keywords": [
    "consciousness",
    "ai",
    "kubernetes",
    "proxmox",
    "vrchat",
    "hoyoverse",
    "character-analysis",
    "federation",
    "coreflame"
  ],
